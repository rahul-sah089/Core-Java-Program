package usingMap;

class Dog {
	public String name;

	public Dog(String name) {
		this.name = name;
	}

	public boolean equals(Object o) {
		if ((o instanceof Dog) && (((Dog) o).name == name)) {
			return true;
		} else
			return false;
	}

	public int hashCode() {
		return name.length();
	}
}

class Cat {
}

enum Pets {
	DOG, CAT, HORSE
};

public class UsingMap {

}
